{
  "version": 2,
  "buildCommand": "echo 'Static site, no build needed'",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=0" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
} 